{
    "name": "mauretto78/in-memory-list",
    "type": "library",
    "description": "In Memory List",
    "keywords": ["apcu", "memcached", "redis", "cache", "list", "in-memory"],
    "homepage": "https://github.com/mauretto78/in-memory-list",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/mauretto78/in-memory-list/issues"
    },
    "authors": [
        {
            "name": "Mauro Cassani",
            "email": "assistenza@easy-grafica.com",
            "homepage": "https://github.com/mauretto78"
        }
    ],
    "autoload": {
        "psr-4": {
            "InMemoryList\\": "src/InMemoryList",
            "InMemoryList\\Command\\": "src/Command"
        }
    },
    "autoload-dev": {
        "psr-4": { "InMemoryList\\Tests\\": "tests/" }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "ramsey/uuid": "^3.6",
        "symfony/console": "^3.2|^4.0",
        "symfony/yaml": "^3.3|^4.0",
        "doctrine/annotations": "1.2.*",
        "doctrine/instantiator": "1.0.*",
        "mauretto78/array-query": "^1.0"
    },
    "require-dev": {
        "predis/predis": "^1.1",
        "phpunit/phpunit": "~5.7",
        "friendsofphp/php-cs-fixer": "^2.3"
    },
    "suggest": {
        "ext-apcu": "*",
        "ext-memcached": "*",
        "ext-redis": "*",
        "predis/predis": ""
    }
}
